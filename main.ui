       <property name="styleSheet">
        <string notr="true">/* --- BLOK CSS SKOPIOWANY Z GLOBALNEGO --- */

/* Tło centralnej sekcji powinno być jasne i neutralne */
/*
QWidget {
    background-color: #F8F9FA;
}
*/

/* Ogólny styl dla etykiet (Label) */
QLabel {
    color: #1b2735; /* Ciemny, czytelny tekst */
}

/* Styl dla głównego nagłówka strony (np. lbl_data_management) */
QLabel#lbl_stfn_analysis {
    font-size: 24px; 
    font-weight: bold;
    padding: 10px 0;
}

/* KONTENERY GRUPUJĄCE (QGroupBox) */

QGroupBox {
    color: #4A89C9; 
    font-weight: bold;
    border: 1px solid #c3cedb;
    border-radius: 4px;
    margin-top: 10px; 
    padding: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px;
    color: #4A89C9; 
    font-size: 12px;
}

/* POLA WEJŚCIA DANYCH (QTextEdit, QPlainTextEdit) */

QTextEdit, QPlainTextEdit {
    background-color: #FFFFFF;
	color: #1b2735;
    border: 1px solid #c3cedb;
    border-radius: 4px;
    padding: 5px;
    /* Ograniczenie wysokości dla spójności z przyciskami (domyślny dla Inputów) */
    max-height: 30px;
}
QTextEdit#txt_bounds, QPlainTextEdit#txt_bounds_data {
    /* Dla pola na Bounds potrzebujemy więcej miejsca */
    max-height: 70px;
	color: #1b2735;
}

QWidget#stfn_page QComboBox {
    background-color: #FFFFFF;
	color: #1b2735;
    border: 1px solid #c3cedb;
    border-radius: 4px;
    padding: 5px;
    min-height: 25px;
}


/* PRZYCISKI AKCJI (Primary Buttons) */

QPushButton#btn_generate_bounds, QPushButton#btn_calculate_stfn {
    background-color: #4A89C9;
    color: #FFFFFF;
    border: none;
    border-radius: 4px;
    padding: 8px 15px;
    font-weight: 600;
    min-height: 25px;
}

QPushButton#btn_generate_bounds:hover, QPushButton#btn_calculate_stfn:hover {
    background-color: #3c72b0;
}

/* PRZYCISKI NAWIGACYJNE WIZUALIZACJI (Secondary Buttons) */
QGroupBox#stfn_visualization_container QPushButton {
    background-color: #75aadb; /* Łagodniejszy błękit */
    color: #FFFFFF;
    border: 1px solid #3c72b0;
    border-radius: 4px;
    padding: 4px 8px;
    font-weight: 500;
    min-height: 20px;
}
QGroupBox#stfn_visualization_container QPushButton:hover {
    background-color: #619bd1;
}</string>
