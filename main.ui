       <property name="styleSheet">
        <string notr="true">/* --- OGÓLNY KONTEKST DLA STRONY MCDA --- */

/* Ogólny styl dla etykiet (Label) */
QWidget#mcda_page QLabel {
    color: #1b2735; /* Ciemny, czytelny tekst */
}

/* Styl dla głównego nagłówka strony */
QLabel#lbl_mcda_title {
    font-size: 24px; 
    font-weight: bold;
    padding: 10px 0;
}

/* --- KONTENERY GRUPUJĄCE (QGroupBox) --- */

QWidget#mcda_page QGroupBox {
    color: #4A89C9; 
    font-weight: bold;
    border: 1px solid #c3cedb;
    border-radius: 4px;
    margin-top: 10px; 
    padding: 10px;
}

QWidget#mcda_page QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 5px;
    color: #4A89C9; 
    font-size: 12px;
}

/* --- POLA WEJŚCIA DANYCH (QTextEdit i QComboBox) --- */

QWidget#mcda_page QTextEdit {
    background-color: #FFFFFF;
	color: #1b2735;
    border: 1px solid #c3cedb;
    border-radius: 4px;
    padding: 5px;
    /* Ograniczenie wysokości dla spójności */
    max-height: 35px;
}
QWidget#mcda_page QComboBox {
    background-color: #FFFFFF;
	color: #1b2735;
    border: 1px solid #c3cedb;
    border-radius: 4px;
    padding: 5px;
    min-height: 25px;
}


/* --- PRZYCISKI AKCJI (Primary Buttons - np. Calculate Ranking) --- */

QPushButton#btn_calculate_ranking {
    background-color: #4A89C9;
    color: #FFFFFF;
    border: none;
    border-radius: 4px;
    padding: 8px 15px;
    font-weight: 600;
    min-height: 25px;
}

QPushButton#btn_calculate_ranking:hover {
    background-color: #3c72b0;
}

/* --- PRZYCISKI WIZUALIZACJI (Secondary Buttons - np. Rank Comaprison) --- */
QGroupBox#mcda_visualization_container QPushButton {
    background-color: #75aadb; /* Łagodniejszy błękit */
    color: #FFFFFF;
    /* Usunięto 1px solid #3c72b0 na rzecz bardziej płaskiego stylu */
    border: none; 
    border-radius: 4px;
    padding: 4px 8px;
    font-weight: 500;
    min-height: 20px;
}
QGroupBox#mcda_visualization_container QPushButton:hover {
    background-color: #619bd1;
}
QGroupBox#mcda_visualization_container QPushButton:pressed {
    background-color: #538cc4;
}</string>
